{
  "name": "flo-lines-intersections",
  "version": "0.1.0",
  "description": "Simple library for finding lines intersections fast.",
  "private": false,
  "main": "dist/lines-intersections.js",
  "directories": {
    "test": "test"
  },
  "keywords": [
    "lines",
    "line",
    "segement",
    "segments",
    "intersection",
    "intersections",
    "fast"
  ],
  "author": "Floris Steenkamp",
  "license": "MIT",
  "homepage": "http://mat-demo.appspot.com/#!/test-polynomials",
  "devDependencies": {
  	"chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "nyc": "^11.2.1",
    "babel-cli": "^6.22.2",
	"babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babelify": "^7.3.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.5.3",
    "browserify": "^13.1.0",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^2.0.0",
    "transform": "^1.1.2",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "vinyl-transform": "^1.0.0"
  },
  "scripts": {
    "build": "cd build && gulp",
    "test-only": "mocha --compilers js:babel-core/register ./test/js",
    "test": "npm run build && npm run test-only",
    "coverage": "./node_modules/.bin/nyc --reporter=html --reporter=text mocha --compilers js:babel-core/register ./test/js",
    "jsdoc": "cd docs && jsdoc --template jsdoc-json --destination doc.json -c conf.json",
    "jsdoc-copy": "call copy .\\docs\\doc.json ..\\mat_demo\\war\\html\\lines_intersections\\jsdoc.json",
    "jsdoc-all": "npm run jsdoc && npm run jsdoc-copy",
    "build-copy-main": "call copy .\\dist\\flo-lines-intersections.js  ..\\mat_demo\\war\\bower_components\\flo-lines-intersections\\dist",
    "build-copy-min": "call copy .\\dist\\flo-lines-intersections.min.js ..\\mat_demo\\war\\bower_components\\flo-lines-intersections\\dist",
    "build-copy-map": "call copy .\\dist\\flo-lines-intersections.min.js.map ..\\mat_demo\\war\\bower_components\\flo-lines-intersections\\dist",
    "build-copy": "npm run build-copy-main && npm run build-copy-min && npm run build-copy-map",
    "build-all": "npm run build && npm run build-copy"
  }
}
